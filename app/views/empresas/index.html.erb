<p id="notice"><%= notice %></p>

<h1>Empresas</h1>

<table>
  <thead>
    <tr>
      <th>Nomeempresa</th>
      <th>Cnpjempresa</th>
      <th>Tipoempresa</th>
      <th>Statusempresa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.nomeEmpresa %></td>
        <td><%= empresa.cnpjEmpresa %></td>
        <td><%= empresa.tipoEmpresa %></td>
        <td><%= empresa.statusEmpresa %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
