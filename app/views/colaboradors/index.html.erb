<p id="notice"><%= notice %></p>

<h1>Colaboradors</h1>

<table>
  <thead>
    <tr>
      <th>Nomecolaborador</th>
      <th>Cpfcolaborador</th>
      <th>Ctpscolaborador</th>
      <th>Tipocolaborador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @colaboradors.each do |colaborador| %>
      <tr>
        <td><%= colaborador.nomeColaborador %></td>
        <td><%= colaborador.cpfColaborador %></td>
        <td><%= colaborador.ctpsColaborador %></td>
        <td><%= colaborador.tipoColaborador %></td>
        <td><%= link_to 'Show', colaborador %></td>
        <td><%= link_to 'Edit', edit_colaborador_path(colaborador) %></td>
        <td><%= link_to 'Destroy', colaborador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Colaborador', new_colaborador_path %>
